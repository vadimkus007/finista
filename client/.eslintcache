   </div>\n                                            <div className=\"form-group\">\n                                                <div className=\"custom-control custom-checkbox small\">\n                                                    <input type=\"checkbox\" className=\"custom-control-input\" id=\"customCheck\" />\n                                                    <label className=\"custom-control-label\" htmlFor=\"customCheck\">Remember\n                                                        Me</label>\n                                                </div>\n                                            </div>\n                                            <button className=\"btn btn-md btn-primary btn-block\" type=\"submit\">Sign in</button>\n                                        </form>\n                                        <hr />\n                                        <div className=\"text-center\">\n                                            <a className=\"small\" href=\"/signup\">Create an Account!</a>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                                {(message.length>0) &&\n                                <div className={'alert alert-danger'}>{message} {error}</div>\n                                }\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n\n        );\n}\n\n\n\n","/home/vadimkus/projects/nodejs/finista/client/src/pages/Signup/index.js",[],"/home/vadimkus/projects/nodejs/finista/client/src/pages/Signup/Signup.js",[],"/home/vadimkus/projects/nodejs/finista/client/src/pages/NotFound/index.js",[],"/home/vadimkus/projects/nodejs/finista/client/src/pages/NotFound/NotFound.js",[],"/home/vadimkus/projects/nodejs/finista/client/src/pages/Quote/index.js",[],"/home/vadimkus/projects/nodejs/finista/client/src/pages/Quote/Quo